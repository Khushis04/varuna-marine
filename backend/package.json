{
  "name": "fuel-eu-backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/infrastructure/server/index.ts",
    "build": "tsc -p .",
    "start": "node dist/infrastructure/server/index.js",
    "migrate": "psql $DATABASE_URL -f src/infrastructure/db/migrations/001_init.sql",
    "seed": "psql $DATABASE_URL -f src/infrastructure/db/seeds/seed.sql"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.7.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
